import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
         int l;
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
           a[i]=s.nextInt(); 
        }
       l=a[0];
    for(int i=1;i<n;i++)
    {
     if(l<a[i])
     l=a[i];
    }
System.out.println(l);
 }
}
