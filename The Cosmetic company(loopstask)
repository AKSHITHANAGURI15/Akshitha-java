import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a=9;
        while(n!=0)
     {
           int r=n%10;         
            if(r<a)
            {
                a=r;
            }
            n=n/10;
         }
        System.out.println(a);
}
}
