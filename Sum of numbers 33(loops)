import java.io.*;
import java.util.*;
public class Solution 
{
public static void main(String[] args) 
{  
    int s1=0;
    Scanner s=new Scanner(System.in);
    while(true)
    {
        int n=s.nextInt();
    { 
         if(n==-1)
         {
             break;
          }
           s1=s1+n;
       }  
    }
      System.out.println(s1);
   }
 }
