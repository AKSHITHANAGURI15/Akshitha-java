#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{  
    int t;
    scanf("%d",&t);
    while(t>0)
    {
    int l,n;
    scanf("%d",&l);
    scanf("%d",&n);
    int a[l],i;
    for(i=0;i<l;i++)
    {
        scanf("%d",&a[i]);
    }
    int c=0;
    for(i=0;i<l-1;i++)
    {
        for(int j=i+1;j<l;j++)
        {
            int t=a[i]-a[j];
            if(t<0)
                t=-t;
        if(t==n)
        {
             printf("1\n");
            c=1;
            break;
        }
        }
    }
    if(c==0)
        printf("-1\n");
    t--;
    }
   return 0;
}
