import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int b=a/4;
        int c=a%4;
        int e=b+c;
        if(a<=100||a>=200)
        {
            System.out.println("INVALID INPUT");
        }
        else
        {
            System.out.printf("%d\n%d\n%d\n%d",b,b,b,e);
        }
    }
}
