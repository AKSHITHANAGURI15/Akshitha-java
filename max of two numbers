import java.io.*;
import java.util.*;
public class Solution 
{
 public static void main(String[] args) 
 {
     int a,b;
     Scanner s=new Scanner(System.in);
     a=s.nextInt();
     b=s.nextInt();
     if(a<b)
     { 
     System.out.println(b);
   }
     else
     {
         System.out.println(a);
     }
 }
}
