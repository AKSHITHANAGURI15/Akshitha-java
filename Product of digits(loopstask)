import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        int d,s1=1;
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        while(n>0)
        {
            d=n%10;
            if(d!=0)
            {
                s1=s1*d;
            }
            n=n/10;
         }
        System.out.println(s1);
        
    }
}
